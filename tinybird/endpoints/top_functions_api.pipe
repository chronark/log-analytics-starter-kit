NODE endpoint
SQL >

    %
    SELECT function_name, countMerge(total)
    FROM per_function_calls_per_hour
    WHERE
        1
        {% if defined(date_from) and defined(date_to) %}
            AND hour
            BETWEEN {{
                DateTime(
                    date_from,
                    "2020-09-09 10:10:10",
                    description="Starting date range",
                    required=True,
                )
            }}
            and {{
                DateTime(
                    date_to,
                    "2020-09-09 10:10:10",
                    description="Starting date range",
                    required=True,
                )
            }}
        {% else %} {{ error("Must supply date_from & date_to params") }}
        {% end %}
    GROUP BY function_name
    LIMIT 10


